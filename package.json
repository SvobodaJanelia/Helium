    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.4.1",
    "core-js": "^2.5.6",
    "express": "^4.16.3",
    "express-list-endpoints": "^3.0.1",
    "express-paginate": "^1.0.0",
