{
  "name": "helium",
  "version": "0.0.1",
  "description": "Svoboda Lab experiment metadata collection and standardization",
  "scripts": {
    "build": "gulp build",
    "dev": "gulp --watch",
    "e2e": "yarn e2e:prep && concurrently --kill-others \"yarn start\" \"yarn webdriver-manager start\" \"sleep 3 && protractor client/config/protractor.conf.js\" --prefix-colors green,blue,magenta --names http,selenium,protractor --prefix name --success first",
    "e2e:live": "yarn e2e:prep && concurrently --kill-others \"yarn start\" \"yarn webdriver-manager start\" \"sleep 3 && protractor client/config/protractor.conf.js --elementExplorer\" --prefix-colors green,blue,magenta --names http,selenium,protractor --prefix name --success first",
    "e2e:prep": "webdriver-manager update",
    "start": "node dist",
    "test": "gulp testPrep && npm run test:server && npm run test:client && gulp build && npm run e2e",
    "test:client": "karma start client/config/karma.conf.js",
    "test:server": "gulp server:testPrep && TS_NODE_PROJECT=server/tsconfig.json nyc mocha --opts server/test/mocha.opts"
  },
  "author": "Matthew Dean <deanmatthew16@gmail.com>",
  "repository": "thatJavaNerd/Helium",
