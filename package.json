{
  "name": "helium",
  "version": "0.0.1",
  "description": "Svoboda Lab experiment metadata collection and standardization",
  "scripts": {
    "build": "gulp build && ng build",
    "dev": "gulp build && concurrently --raw \"nodemon dist\" \"gulp watch\" \"ng build --watch\"",
    "e2e": "yarn build && concurrently --raw --kill-others \"PORT=4200 node dist\" \"sleep 1 && yarn e2e:prepped\"",
    "e2e:debug": "yarn build && concurrently --raw --kill-others \"PORT=4200 node dist\" \"sleep 1 && yarn e2e:prepped -- -ee\"",
    "e2e:prepped": "ng e2e --serve false",
    "e2e:prepped:debug": "ng e2e --serve false -ee",
    "prod": "gulp build && ng build --prod && node dist",
    "start": "node dist",
    "test": "yarn test:server && yarn test:client && yarn e2e",
    "test:server": "gulp server:testPrep && TS_NODE_PROJECT=server/tsconfig.json nyc mocha --opts server/test/mocha.opts",
    "test:client": "ng test --single-run true",
    "test:client:live": "ng test --single-run false"
  },
  "author": "Matthew Dean <deanmatthew16@gmail.com>",
  "repository": "thatJavaNerd/Helium",
  "engines": {
    "node": "7.10.0"
  },
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/cli": "^1.2.4",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@swimlane/ngx-datatable": "^9.3.1",
    "body-parser": "^1.17.1",
    "chalk": "^2.0.1",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "express-list-endpoints": "^3.0.0",
    "express-paginate": "^0.3.0",
    "helmet": "^3.8.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "mysql2": "^1.3.6",
    "ng-inline-svg": "^3.3.0",
    "normalize.css": "^7.0.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2",
    "squel": "^5.11.1",
    "yargs": "^8.0.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/chai": "^4.0.0",
    "@types/chai-as-promised": "^0.0.31",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^2.2.40",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.17",
    "@types/supertest": "^2.0.2",
    "@types/yargs": "^8.0.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.4.0",
    "del": "^3.0.0",
    "env-test": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-about": "^1.1.0",
    "gulp-typescript": "^3.2.1",
    "istanbul": "1.1.0-alpha.1",
